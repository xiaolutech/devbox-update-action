{
  "name": "devbox-updater-action",
  "version": "1.0.0",
  "description": "GitHub Action to automatically update Devbox packages",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist --license licenses.txt",
    "package": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "npm run build",
    "all": "npm run lint && npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaolutech/devbox-update-action.git"
  },
  "keywords": [
    "devbox",
    "github-action",
    "package-updater",
    "nix"
  ],
  "author": "Devbox Community",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@actions/core": "^1.11.0",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/io": "^1.1.3",
    "axios": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/jest": "^29.5.8",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^9.0.0",
    "fast-check": "^3.22.0",
    "jest": "^30.0.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.9.0"
  }
}
